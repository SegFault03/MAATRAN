<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maatran">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mumtranlogin" >
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <activity
            android:name="com.example.maatran.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.maatran.RegistrationActivity">
        </activity>
        <activity android:name="com.example.maatran.LoginActivity">
        </activity>
        <activity android:name="com.example.maatran.EmailPasswordActivity">
        </activity>
        <activity android:name="com.example.maatran.EmailSignUp">
        </activity>
        <activity android:name=".DashboardActivity" />
        <activity android:name=".DetailsActivity" />
    </application>

</manifest>