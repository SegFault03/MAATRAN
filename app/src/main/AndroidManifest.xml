<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Maatran">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/maatran_icon_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mumtranlogin">
        <activity
            android:name="com.example.Maatran.ProfileView"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="com.example.Maatran.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Maatran.RegistrationActivity"></activity>
        <activity android:name="com.example.Maatran.LoginActivity"></activity>
        <activity android:name="com.example.Maatran.EmailPasswordActivity"></activity>
        <activity android:name="com.example.Maatran.EmailSignUp"></activity>
        <activity android:name="com.example.Maatran.DashboardActivity" />
        <activity android:name="com.example.Maatran.DetailsActivity" />
    </application>

</manifest>